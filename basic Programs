import java.util.Scanner;
public class Test {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b= sc.nextInt();
        int[][] arr = new int [a] [b];
        char ch=97;
        for (int i=0;i<a;i++) {
            for (int j=0;j<b;j++) {
                if (i==1 && j==0 || i==1 && j==2) {
                    System.out.print("");
                }
                else {
                    System.out.print(ch);
                    ch++;
                }
            }
            System.out.println();
        }
        
    }
}
