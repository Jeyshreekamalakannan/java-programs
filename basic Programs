discount program:


public class Car {
    String model;
    int price;
Car (String model,int price) {
    this.model = model;
    this.price = price;

}
void display() {
    System.out.println(model +" " + price);
    discount(price);
} 
void discount(int price) {
    price = price - (price*25/100);
   
    System.out.println(price);
}
public static void main(String[] args) {
    Car obj = new Car( "Benz" , 23900);
    obj.display();
}
}

